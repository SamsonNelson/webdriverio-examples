{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Mocha test",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/googleSearch_tests/node_modules/webdriverio/bin/wdio",
            "stopOnEntry": true,
            "args":[
                "${workspaceRoot}/googleSearch_tests/wdio.conf.js"
            ],
            "cwd": "${workspaceRoot}/googleSearch_tests",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        }
    ]
}